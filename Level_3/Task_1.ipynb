    "class Person:\n",
    "    def __init__(self, name):\n",
    "        self.name = name\n",
    "    def greet(self):\n",
    "        return f\"Hi, {self.name}\"\n",
    "person1 = Person(\"Alice\")\n",
    "greeting = person1.greet()\n",
    "print(greeting)"
